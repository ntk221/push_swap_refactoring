#!/bin/bash

main() {
    echo "-------------- system test ---------------"
    valgrind ./push_swap 1 2 3
    valgrind ./push_swap 3 2 1
    ./push_swap 5 4 3 2 1 # valgrind と実行すると無限ループ！！！！
    valgrind ./push_swap -1 1
    valgrind ./push_swap 0 0
    ./push_swap 4 3 2 1   # valgrind と 実行すると無限ループ
    # ./push_swap "3 2 1" #未実装
    valgrind ./push_swap 4 -100   3 50 55 100
    # ./push_swap 3 4 5 2 1
    valgrind ./push_swap -1365692127 -2078284736 -1016755303 1499288138 -2000186829 880294176 -1227220932 320336692 231391443 -1688140880 -188260278 1514688411 250589046 509250624 -1015507561 -1487968797 2088840660 -1847171876 22713578 -238246567 -830262207 530863553 -279094490 862924490 -1372731960 -570673183 -1595180745 1666615205 -354102629 -1999523177 2013494764 1882228318 -1362333374 -2137654856 1637040521 1244287697 -259575804 2088473535 1145751855 1908373360 1639170447 2054471011 1725053787 1556833712 2015145478 974125480 -1531649310 -1312945740 -1027182042 2070800567 -60913912 503373999 1446292088 -101096980 -1109304580 1788379278 981801554 428113383 -1099282736 1356180331 -339866883 -1232833650 1801215252 -1966801960 -1584877943 -142473695 1048904187 391378683 -1520251550 327064532 -468853564 -548581670 -561617554 -48315363 2092529479 210328334 -1043134249 -1578311951 912045332 -2040872845 1543170567 32205424 671697593 -351524976 -1356021923 -713682162 -403614692 -862237965 -761191376 2056541047 1438829838 195411680 624988114 1438871311 -625365241 -621200455 -547724077 434104083 367042644 -1167340893
    ./push_swap -2147483649 # Error
    echo "-------------- finish system test ----------------"
}

main;
exit 0
